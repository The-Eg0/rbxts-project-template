{
	"name": "template",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"$path": "out/Server"
		},

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				}
			},
			"Modules": {
				"$path": "out/Shared"
			},

			"Client": {
				"$className": "Folder",
				"$path": "out/Client"
			},

			"UI": {
				"$className": "Folder",
				"$path": "out/UI"
			},

			"Stories": {
				"$className": "Folder",
				"$path": "out/UI/Stories",

				"$properties": {
					"Archivable": true
				}
			}
		}
	}
}
